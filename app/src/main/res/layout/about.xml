<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/licenseInfo"
        android:layout_marginTop="2dp"
        android:layout_below="@+id/setLicense"
        android:layout_marginLeft="11dp"
        android:paddingLeft="11dp"
        android:textSize="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/setLicense"
        android:layout_marginTop="6dp"
        android:paddingLeft="11dp"
        android:textColor="#060606"
        android:textStyle="bold|italic"
        android:textSize="15dp"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/licenseInfo"
        android:layout_alignStart="@+id/licenseInfo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/note"
        android:paddingLeft="10dp"
        android:textColor="#060606"
        android:textSize="15dp"
        android:layout_below="@+id/aboutInfo"
        android:layout_alignLeft="@+id/aboutInfo"
        android:layout_alignStart="@+id/aboutInfo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    Licenses"
        android:id="@+id/textView6"
        android:textColor="#1e059c"
        android:textStyle="bold|italic"
        android:typeface="sans"
        android:layout_marginTop="9dp"
        android:layout_below="@+id/note"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/aboutInfo"
        android:paddingLeft="10dp"
        android:textColor="#060606"
        android:textSize="15dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/setLicense"
        android:layout_alignStart="@+id/setLicense"
        android:layout_marginTop="10dp" />

</RelativeLayout>
